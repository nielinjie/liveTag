<ivysettings>
    <settings defaultResolver="public"/>
    <resolvers>
        <chain name="public" dual="true">
            <filesystem name="local">
                <artifact pattern="/home/nielinjie/lib/sun/[module]-[revision].[ext]" />
            </filesystem>
            <ibiblio name="snapshots.codehaus" m2compatible="true" root="http://snapshots.repository.codehaus.org/"/>
            <ibiblio name="codehaus" m2compatible="true" root="http://repository.codehaus.org/"/>
            <ibiblio name="ibib" m2compatible="true"/>
            <url name="exist.com" m2compatible="true">
                <ivy pattern="http://repo.exist.com/maven2/[organisation]/[module]/[revision]/[module]-[revision].pom" />
                <artifact pattern="http://repo.exist.com/maven2/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
            </url>
            <url name="com.springsource.repository.bundles.release" m2compatible="true">
                <ivy pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                <artifact pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
            </url>
            <url name="com.springsource.repository.bundles.external" m2compatible="true">
                <ivy pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                <artifact pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
            </url>          
            <ibiblio name="java.net" m2compatible="true" root="http://download.java.net/maven/1/" pattern="[organization]/jars/[module]-[revision].[ext]" />
            
        </chain>
    </resolvers>
</ivysettings>
